sket\n    // .3 send the id to the reducer -> the reducer will remove the product by id\n    dispatch({\n      type: \"REMOVE_FROM_BASKET\",\n      id: id,\n    });\n  };\n\n  return (\n    <div className=\"checkoutProduct\">\n      <img className=\"checkoutProduct__image\" src={image} alt=\"\" />\n      <div className=\"checkoutProduct__info\">\n        <p className=\"checkoutProduct__title\">{title}</p>\n        <p className=\"checkoutProduct_price\">\n          <small>$</small>\n          <p>{price}</p>\n        </p>\n        <div className=\"checkoutProduct__ranting\">\n          {Array(rating)\n            .fill()\n            .map((_, i) => (\n              <p>⭐️</p>\n            ))}\n        </div>\n        <button onClick={removeFromBasket}>Remove From basket</button>\n      </div>\n    </div>\n  );\n}\n\nexport default CheckoutProduct;\n","/Users/silasip/Desktop/Silas/React_proejct/amazon-clone/src/index.js",[],["63","64"],"/Users/silasip/Desktop/Silas/React_proejct/amazon-clone/src/StateProvider.js",[],"/Users/silasip/Desktop/Silas/React_proejct/amazon-clone/src/App.js",[],"/Users/silasip/Desktop/Silas/React_proejct/amazon-clone/src/reducer.js",[],"/Users/silasip/Desktop/Silas/React_proejct/amazon-clone/src/components/Header/Header.js",[],"/Users/silasip/Desktop/Silas/React_proejct/amazon-clone/src/components/Subtotal/Subtotal.js",[],"/Users/silasip/Desktop/Silas/React_proejct/amazon-clone/src/components/Login/Login.js",[],{"ruleId":"65","severity":1,"message":"66","line":4,"column":8,"nodeType":"67","messageId":"68","endLine":4,"endColumn":24},{"ruleId":"65","severity":1,"message":"69","line":5,"column":8,"nodeType":"67","messageId":"68","endLine":5,"endColumn":27},{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"65","severity":1,"message":"74","line":6,"column":12,"nodeType":"67","messageId":"68","endLine":6,"endColumn":18},{"ruleId":"65","severity":1,"message":"75","line":8,"column":22,"nodeType":"67","messageId":"68","endL